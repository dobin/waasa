<Window x:Class="waasa.UI.WindowDownload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:waasa.UI"
        mc:Ignorable="d"
        Title="WindowDownload" Height="450" Width="800">
    <Grid>
        <Button Click="ButtonDownload"  Content="Download" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="-0.147,0.578"/>
        <TextBox HorizontalAlignment="Center" Margin="0,35,0,0" TextWrapping="Wrap" Text="{Binding OutputTextBox}" VerticalAlignment="Top" Width="780" Height="399"/>

    </Grid>
</Window>
